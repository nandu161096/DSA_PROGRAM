
#include <stdio.h>
#include <string.h>


int main() {
    char str[] = "aabbbc";
    int freq[256] = {0};
    int i = 0;
    printf("%s\n",str);
    for(i = 0; str[i] !='\0'; i++) {
        freq[(unsigned char)str[i]]++;
    }
  
    for(i = 0; i < 256; i++) {
        if (freq[i]!=0) {
            printf("freq[%c]:%d \n",i,freq[i]);
        }
    }
}
