#include <stdio.h>
#include <string.h>

void reverse(char str[]) {
    int len = strlen(str) - 1;
    int l = 0, r = len;
    while(l < r) {
        char temp = str[l];
        str[l++] = str[r];
        str[r--] = temp;
    }
    
}
int main() {
    char str[] = "hello";
    reverse(str);
    printf("%s\n", str); // olleh
}
