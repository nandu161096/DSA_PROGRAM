#include <stdio.h>

int fib(int n) {
    
    if (n == 0) {
        return 0;
    }
    
    if (n == 1)  {
        return 1;
    }
    
    return fib(n-1) + fib(n-2);
}

int main()
{
    int fibo = fib(6);
    printf("Hello World %d",fibo);
    return 0;
}
