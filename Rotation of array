#include <stdio.h>

void reverse_array(int arr[], int l, int r) {
    while(l < r) {
        int temp = arr[l];
        arr[l++] = arr[r];
        arr[r--] = temp;
    }
}

void print_array(int arr[], int n) {
    int i = 0;
    for(i = 0; i < n;i++) {
        printf("arr[%d] %d ",i,arr[i]);
    }
    printf("\n");
}
void rotate(int arr[], int n, int k) {
    k = k % n;
    reverse_array(arr, 0, n-1);
    print_array(arr,n);
    reverse_array(arr, k-1, n-1);  
    print_array(arr,n);
    reverse_array(arr, 0, k);
    print_array(arr,n);
}

int main() {
    int arr[] = {1, 2, 3, 6, 5};
    int n = sizeof(arr) / sizeof(arr[0]);
    print_array(arr, n);
    rotate(arr, n,3);

    return 0;
}
