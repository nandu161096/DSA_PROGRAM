#include <stdio.h>
#include <stdlib.h>

struct node {
    int data;
    struct node *next;
};

int main()
{
    int i = 0, n = 0;
    int arr[] = {1,3,5,7,9};
    n = sizeof(arr)/sizeof(arr[0]);
    struct node *head = NULL, *temp = NULL, *prev = NULL;
    
    for(i = 0; i < n;i++) {
        temp = (struct node*)malloc(sizeof(struct node));
        temp->data = arr[i];
        temp->next = NULL;
        if (head == NULL) {
            head = temp;
        } else {
            prev->next = temp;
        }
        prev = temp;
    }
    printf("prev %d\n",prev->data);
    prev->next = head;
    
    temp = head;
    do {
        printf("%d ",temp->data);
        temp = temp->next;
    } while (temp != head);
    return 0;
}
