#include <stdio.h>
#include <stdlib.h>

char *my_strcpy(char *dest, char *src) {
    char *orig_dst = dest;
    
    while(*src!= '\0') {
        *dest++ = *src++;
    }
    
    *dest == '\0';
    printf("orig %p dest %p addr orig_dst %p dest %p\n",orig_dst,dest,&dest,&orig_dst);
    return orig_dst;
}

int my_strcmp(const char *str1, const char *str2) {
    
    unsigned char *p1 = (unsigned char*)str1;
    unsigned char *p2 = (unsigned char*)str2;
    
    while((*p1 == *p2) && (*p1 != '\0')) {
        p1++;
        p2++;
    }
    
    return (int)*p1 - (int)*p2;
}

int main() {
    char str[] = "abcdef";
    int i = 0, len = 0;
    for(i = 0; str[i] != '\0';i++) {
        len++;
    }
    printf("%d\n",len);
    char dst[60];
    my_strcpy(dst, str);
    dst[0] = 'a';
    printf("str %s dst %s addr %p\n",str,dst,&dst);
    int ret = my_strcmp(str, dst);
    printf("ret %d\n",ret);
    
}
