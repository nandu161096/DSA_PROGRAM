#include <stdio.h>

int missing(int arr[], int n) {
    int x = 0, i = 0;
    for(i = 0;i<n;i++) {
        x = x^arr[i];
    }
    for(i = 1;i<=n+1;i++) {
        x = x^i;
    }
    return x;
}

int main() {
    int arr[] = {1, 2, 3, 6, 5};
    int n = sizeof(arr) / sizeof(arr[0]);
    int missing_num = missing(arr, n);

    printf("Missing number is: %d\n", missing_num);

    return 0;
}
