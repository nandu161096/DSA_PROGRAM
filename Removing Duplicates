#include <stdio.h>
#include <string.h>

void removeDuplicates(char str[])
{
    int seen[256] = {0}, i = 0, idx = 0;
    
    for(i = 0; str[i];i++) {
        if (!seen[(unsigned char)str[i]]) {
            seen[(unsigned char)str[i]] = 1;
            str[idx++] = str[i];
        }
    }
    str[idx] = '\0';
}

int main() {
    char str[] = "programming";
    removeDuplicates(str);
    printf("%s\n", str); // progamin
}
