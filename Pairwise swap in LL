#include <stdio.h>
#include <stdlib.h>

struct node {
    int data;
    struct node *next;
};

void print_list(struct node *head)
{
    while (head) {
        printf("%d -> ", head->data);
        head = head->next;
    }
    printf("NULL\n");
}

struct node *rotate(struct node *head, int k) {
    if (!head || (k ==0)) {
        printf("inside rotate\n");
        return head;
    }
    struct node *curr = NULL, *kth = NULL;
    curr = head;
    printf("inside rotate\n");
    
    int count = 1;
    while(count < k) {
        curr = curr->next;
        count++;
    }
    kth = curr;
    
    while(curr->next) {
        curr = curr->next;
    }
    
    printf("kth node %d curr %d\n",kth->data, curr->data);
    curr->next = head;
    head = kth->next;
    kth->next = NULL;
    
    return head;
}

struct node* pairwise(struct node *head) {
    struct node* curr = NULL, *prev = NULL, *next = NULL, *next_next = NULL;
    curr = head;
    head = curr->next;
    
    while(curr && curr->next) {
        next = curr->next;
        next_next = next->next;
        
        next->next = curr;
        curr->next = next_next;
        
        if (prev) {
            prev->next = next;
        }
        
        prev = curr;
        curr = next_next;
    }
    return head;
}

int main()
{
    int i;
    struct node *head1 = NULL, *head2 = NULL;
    struct node *prev = NULL, *temp = NULL;
    struct node *intersection = NULL;
    
    int arr[] = {1, 2, 3, 4, 5};
    int arr1[] = {2,4,6,8,10};
    int n  = sizeof(arr) / sizeof(arr[0]);
    int n1 = sizeof(arr1) / sizeof(arr1[0]);

    /* Create first list */
    prev = NULL;
    for (i = 0; i < n; i++) {
        temp = malloc(sizeof(struct node));
        temp->data = arr[i];
        temp->next = NULL;

        if (!head1)
            head1 = temp;
        else
            prev->next = temp;

        prev = temp;
    }

    printf("List 1:\n");
    print_list(head1);
    head1 = rotate(head1,3);
    printf("After rotate List 1:\n");
    print_list(head1);
    head1 = pairwise(head1);
    printf("After swap List 1:\n");
    print_list(head1);
    return 0;
}
