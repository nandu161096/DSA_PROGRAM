
void remove_dup(char *str) {
    int visit[256] = {0};
    int ind = 0;
    int i = 0;
    for(i = 0; str[i] != '\0';i++) {
        if (!visit[(unsigned char)str[i]]) {
            visit[(unsigned char)str[i]] = 1;
            str[ind++] = str[i];
        }
    }
    str[ind] = '\0'; 
}

int main() {
    char str[] = "thrrrrroooow";
    char dst[] = "worth";
    printf("before %s\n",str);
    remove_dup(str);
    printf("after %s\n",str);
    return 0;
}
